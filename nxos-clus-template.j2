feature nxapi
feature telemetry

telemetry
  destination-profile
    use-vrf management
  destination-group 100
    ip address {{ pipeline }} port 57500 protocol gRPC encoding GPB
  sensor-group 200
    data-source NX-API
    path "show system resources"
    path "show interface mgmt0"
    path "show bgp vrf all sessions"
    path "show ip ospf neighbors"
  subscription 200
    dst-grp 100
    snsr-grp 200 sample-interval 20000
